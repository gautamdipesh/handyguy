<!DOCTYPE html>
<html lang="en">
<head>
    <title></title>
   <% include ./partials/head %>
</head>

<body>
  <div id="wrapper">
    <% include ./partials/navbar %>
      <div class="page-wrapper">
        <div class="container jumbotron">
              <h2 class="text-center">My Profile</h2>
            <form id="user_profile" role="form" action="/userprofile" class="form-horizontal col-lg-12">
            <fieldset>
                <!-- User Profile form -->
                <legend>Business Information</legend>
                <!-- Business Name input-->
                <div class="form-group">
                    <label for="business_name" class="control-label col-md-2">Business Name</label>
                    <!-- Place Holder for Business Name-->
                    <div class="col-md-10">
                        <p class="lead"></p>
                    </div>
                </div>
                <!-- Phone input-->
                <div class="form-group">
                    <label for="business_tel" class="control-label col-md-2">Telephone Number</label>
                    <div class="col-md-10">
                        <div class="col-md-10">
                        <p class="lead"></p>
                    </div>
                    </div>
                </div>

                <!-- email input-->
                <div class="form-group">
                    <label for="business_email" class="control-label col-md-2">Email</label>
                    <div class="col-md-10">
                        <p class="lead"></p>
                    </div>
                </div>
                
                <!-- address-line1 input-->
                <div class="form-group">
                    <label for="business_add1" class="control-label col-md-2">Address Line 1</label>
                    <div class="col-md-10">
                        <p class="lead"></p>
                    </div>
                </div>
                <!-- address-line2 input-->
                <div class="form-group">
                    <label for="business_add2" class="control-label col-md-2">Address Line 2</label>
                    <div class="col-md-10">
                        <p class="lead"></p>
                    </div>
                </div>
                <!-- city input-->
                <div class="form-group">
                    <label for="business_city"class="control-label col-md-2">City</label>
                    <div class="col-md-10">
                        <p class="lead"></p>
                    </div>
                </div>
                <!-- Region input-->
                <div class="form-group">
                    <label for="business_state" class="control-label col-md-2">State</label>
                    <div class="col-md-10">
                        <p class="lead"></p>
                    </div>
                </div>
                <!-- postal-code input-->
                <div class="form-group">
                    <label for="business_zip" class="control-label col-md-2">Zip Code</label>
                    <div class="col-md-10">
                        <p class="lead"></p>
                    </div>
                </div>
                <!-- country select -->
                <div class="form-group">
                    <label for="business_country" class="control-label col-md-2">Country</label>
                    <div class="col-md-10">
                        <p class="lead"></p>
                    </div>
                </div>
               
                <!-- CompanyLogo input-->
                <div class="form-group">
                    <label for="business_logo" class="control-label col-md-2">Logo</label>
                    <div class="col-md-10">
                      <p class="lead"></p>
                      </div>
                </div>
                
              
                <div class="form-group">
                    <div class="col-md-10 col-md-offset-2">
                      <button type="Edit" class="btn btn-raised btn-warning">Edit</button>
                      <button id="submit_btn" type="submit" value="submit" class="btn btn-raised btn-success">Save</button>
                    </div>
                </div>
          </fieldset>
    </form>
    </div><!--Jumbotron -->
    </div><!--page-wrapper -->
    </div><!-- container -->
    </div> <!-- wrapper -->
<!-- Scripts -->
<% include ./partials/scriptsloader %>
</body>
</html>